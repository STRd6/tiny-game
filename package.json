    "@types/mocha": "^9.1.1",
    "browserify": "^17.0.0",
    "canvas": "^2.9.0",
    "coffeescript": "2.6.1",
    "esbuild": "^0.14.38",
    "jsdom": "^19.0.0",
    "mocha": "^9.2.0",
    "node-fetch": "^2.6.6",
    "nyc": "^15.1.0",
    "peerjs": "^1.3.2",
